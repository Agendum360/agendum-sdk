{
    "name": "agendum/agendum-sdk",
    "description": "Agendum SDK for PHP applications",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Francesco Bianco",
            "email": "bianco@javanile.org"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "pestphp/pest": "^2.0",
        "phpstan/phpstan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Agendum\\Sdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Agendum\\Sdk\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
